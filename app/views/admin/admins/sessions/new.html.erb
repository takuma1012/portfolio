<div id="sign-box">

  <h1>Log in</h1>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field-login">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "double" %>
    </div>

    <div class="field-login">
      <%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "off", class: "double" %>
    </div>

    <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div><br />
    <% end -%>

      <%= f.submit "Log in", class: "login" %>
  <% end %>

  <%= render "admin/admins/shared/links" %>
</div>